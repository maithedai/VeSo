<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="return_stock_form" model="ir.ui.view">
            <field name="name">return.stock.form</field>
            <field name="model">return.stock</field>
            <field name="arch" type="xml">
                <form>
                    <field name="customer_ids">
                        <tree>
                            <field name="name" string="Khách hàng"/>
                            <field name="gender"/>
                            <field name="phone"/>
                            <field name="address"/>
                            <field name="unit_price"/>
                            <field name="status"/>
                            <field name="note"/>
                        </tree>
                    </field>
                </form>
            </field>
        </record>
        <record id="return_stock_action" model="ir.actions.act_window">
            <field name="name">Trả ế</field>
            <field name="res_model">return.stock</field>
            <field name="view_mode">form</field>
        </record>
    </data>
</odoo>